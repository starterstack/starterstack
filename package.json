{
  "name": "starterstack",
  "version": "0.0.21",
  "description": "This project was bootstrapped with starterstack, a Serverless monorepo template architecture",
  "scripts": {
    "setup": "node ./scripts/setup.mjs",
    "patch-files": "bash ./scripts/patch-files.sh",
    "postinstall": "husky install && npm run patch-files",
    "prepare": "husky install && npm run patch-files",
    "docs": "bash ./scripts/docs.sh",
    "test": "bash ./scripts/test.sh",
    "cz": "cz",
    "deploy": "node ./scripts/deploy.mjs",
    "lint:cloudformation": "node ./scripts/lint-cloudformation.mjs",
    "proxy": "node ./scripts/proxy.mjs",
    "create:user": "node ./scripts/create-user.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starterstack/starterstack.git"
  },
  "bugs": {
    "url": "https://github.com/starterstack/starterstack/issues"
  },
  "keywords": [
    "serverless",
    "sam",
    "serverless-application-model",
    "aws",
    "lambda",
    "aws-lambda",
    "aws-lambda-node",
    "aws-lambda-python",
    "cloudformation",
    "dynamodb",
    "api",
    "graphql",
    "reference-architecture",
    "serverless-template",
    "eventbridge",
    "eventbridge-archive",
    "sqs",
    "sns",
    "ses",
    "s3",
    "serverless-rest",
    "serverless-graphql",
    "cloudfront",
    "cloudfront-functions",
    "aws-apigateway",
    "serverless-framework",
    "custom-cloudformation"
  ],
  "author": "James Kyburz <james.kyburz@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 20 < 21",
    "npm": ">= 10"
  },
  "eslintIgnore": [
    "!.github",
    ".github/actions/dist",
    "packages/cdn/cloudfront-viewer-request.js",
    "packages/cdn/cloudfront-viewer-response.js",
    "packages/web/functions/ssr/fetch-data.mjs"
  ],
  "lint-staged": {
    "*.{ts,js,mjs,mts,cjs,mts}": [
      "eslint --fix --no-error-on-unmatched-pattern",
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    ".github/actions/*.js": "bash ./scripts/build-actions.sh",
    "scripts/directory-hash.mjs": "bash ./scripts/build-actions.sh",
    "packages/settings.json": "bash ./scripts/build-actions.sh",
    "packages/shared/*.js": "bash ./scripts/tsc-shared.sh",
    "*.py": "bash ./scripts/lint-python.sh",
    "*.{sh,.bashrc}": "bash ./scripts/lint-sh.sh",
    "*.{md,yaml,json,yml,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "devDependencies": {
    "@actions/github": "6.0.0",
    "@aws-sdk/client-acm": "3.507.0",
    "@aws-sdk/client-api-gateway": "3.509.0",
    "@aws-sdk/client-apigatewaymanagementapi": "3.515.0",
    "@aws-sdk/client-cloudformation": "3.490.0",
    "@aws-sdk/client-cloudfront-keyvaluestore": "3.504.0",
    "@aws-sdk/client-dynamodb": "3.490.0",
    "@aws-sdk/client-eventbridge": "3.511.0",
    "@aws-sdk/client-lambda": "3.490.0",
    "@aws-sdk/client-route-53": "3.499.0",
    "@aws-sdk/client-s3": "3.490.0",
    "@aws-sdk/client-ses": "3.511.0",
    "@aws-sdk/client-sesv2": "3.511.0",
    "@aws-sdk/client-sfn": "3.515.0",
    "@aws-sdk/client-ssm": "3.490.0",
    "@aws-sdk/client-sts": "3.490.0",
    "@aws-sdk/lib-dynamodb": "3.490.0",
    "@aws-sdk/signature-v4-crt": "3.502.0",
    "@aws-sdk/signature-v4-multi-region": "3.502.0",
    "@aws-sdk/util-dynamodb": "3.490.0",
    "@commitlint/cli": "19.2.0",
    "@commitlint/config-conventional": "19.1.0",
    "@sindresorhus/tsconfig": "5.0.0",
    "@starterstack/sam-expand": "0.2.12",
    "@types/aws-lambda": "8.10.131",
    "@types/qrcode-terminal": "0.12.2",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "aws-sdk-client-mock": "3.0.1",
    "aws-xray-sdk-core": "3.5.3",
    "commitizen": "4.3.0",
    "esbuild": "0.20.2",
    "eslint-plugin": "1.0.1",
    "eslint-plugin-unicorn": "51.0.1",
    "express": "4.18.3",
    "http-proxy-middleware": "2.0.6",
    "husky": "9.0.11",
    "inquirer": "9.2.16",
    "lint-staged": "15.2.2",
    "otpauth": "9.2.2",
    "prettier": "3.2.5",
    "prettier-config-standard": "7.0.0",
    "qrcode-terminal": "0.12.0",
    "sls-mentor": "2.4.6",
    "typescript": "5.4.2"
  }
}
